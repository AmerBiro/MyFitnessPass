<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home">
    <fragment
        android:id="@+id/registrationSplashScreen"
        android:name="myfitnesspass.basic.registration.RegistrationSplashScreen"
        android:label="registration_splash_screen"
        tools:layout="@layout/splash_screen" >
        <action
            android:id="@+id/action_registrationSplashScreen_to_home"
            app:destination="@id/home" />
    </fragment>
    <fragment
        android:id="@+id/personal"
        android:name="myfitnesspass.basic.registration.SignUp"
        android:label="registration_personal"
        tools:layout="@layout/auth_sign_up" >
        <action
            android:id="@+id/action_personal_to_home"
            app:destination="@id/home" />
    </fragment>
    <fragment
        android:id="@+id/personal2"
        android:name="myfitnesspass.basic.registration.Personal2"
        android:label="registration_personal2"
        tools:layout="@layout/registration_personal2" />
    <fragment
        android:id="@+id/body"
        android:name="myfitnesspass.basic.registration.Body"
        android:label="registration_body"
        tools:layout="@layout/profile_body" />
    <fragment
        android:id="@+id/membership"
        android:name="myfitnesspass.basic.registration.Membership"
        android:label="registration_membership"
        tools:layout="@layout/profile_membership" />
    <fragment
        android:id="@+id/caoch"
        android:name="myfitnesspass.basic.registration.Caoch"
        android:label="registration_caoch"
        tools:layout="@layout/coach" />
    <fragment
        android:id="@+id/profilePhoto"
        android:name="myfitnesspass.basic.registration.ProfilePhoto"
        android:label="registration_profile_photo"
        tools:layout="@layout/profile_photo" />
    <fragment
        android:id="@+id/home"
        android:name="myfitnesspass.basic.home.Home"
        android:label="main_app_home"
        tools:layout="@layout/home" >
        <action
            android:id="@+id/action_home_to_programCreationI"
            app:destination="@id/programCreationI" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="myfitnesspass.ui.auth.login.fragments.LoginFragment"
        android:label="auth_login"
        tools:layout="@layout/auth_login" />
    <fragment
        android:id="@+id/programCreationI"
        android:name="myfitnesspass.basic.home.programs.program_creation.ProgramCreationI"
        android:label="fragment_program_creation_i"
        tools:layout="@layout/fragment_program_creation_i" >
        <action
            android:id="@+id/action_programCreationI_to_programCreationII"
            app:destination="@id/programCreationII" />
    </fragment>
    <fragment
        android:id="@+id/programCreationII"
        android:name="myfitnesspass.basic.home.programs.program_creation.ProgramCreationII"
        android:label="fragment_program_creation_i_i"
        tools:layout="@layout/fragment_program_creation_i_i" >
        <action
            android:id="@+id/action_programCreationII_to_programCreationIII"
            app:destination="@id/programCreationIII" />
    </fragment>
    <fragment
        android:id="@+id/programCreationIII"
        android:name="myfitnesspass.basic.home.programs.program_creation.ProgramCreationIII"
        android:label="fragment_program_creation_i_i_i"
        tools:layout="@layout/fragment_program_creation_i_i_i" />
</navigation>